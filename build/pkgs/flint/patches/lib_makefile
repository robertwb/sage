LIBDIR=$(PREFIX)/lib
INCLUDEDIR=$(PREFIX)/include
DOCDIR=$(PREFIX)/doc

libflint.dylib: $(FLINTOBJ)
	$(CXX) -single_module -fPIC -dynamiclib -o libflint.dylib $(FLINTOBJ) -lgmp

libflint.dll: $(FLINTOBJ)
	$(CXX) -fPIC -shared -o libflint.dll $(FLINTOBJ) -lgmp

libflint.so: $(FLINTOBJ)
	$(CXX) -fPIC -shared -o libflint.so $(FLINTOBJ) -lgmp
