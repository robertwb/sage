#!/bin/sh -xe
echo "Building Linbox and its wrapper"
mv dist/debian dist/debian-tmp

echo "Build Linbox"
mv linbox src
mv dist/debian-tmp/linbox-debian dist/debian
${SAGE_LOCAL}/bin/sage-build-debian

