#!/usr/bin/env sage-python

import os, sys, socket
from sage.all import *

if len(sys.argv) > 1:
    name = sys.argv[1]
else:
    name = "default_sage_server"

port = 8001

while True:
    try:
        server_http1(name = name, port=port)
        sys.exit(0)
    except socket.error:
        port += 1
